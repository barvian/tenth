{
	"name": "tenth",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:types": "supabase gen types typescript --local > types/supabase.ts",
		"reset:db": "supabase db reset",
		"test:db": "supabase test db",
		"postinstall": "patch-package"
	},
	"devDependencies": {
		"@rollup/plugin-yaml": "^3.1.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "^1.0.0-next.572",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.4",
		"@types/crypto-js": "^4.1.1",
		"@types/lodash": "^4.14.186",
		"@types/plaid-link": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.8",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"mdsvex": "^0.10.6",
		"msw": "^0.46.0",
		"patch-package": "^6.4.7",
		"postcss": "^8.4.16",
		"prettier": "^2.5.1",
		"prettier-plugin-organize-imports": "^3.2.1",
		"prettier-plugin-svelte": "^2.5.0",
		"rimraf": "^3.0.2",
		"supabase": "^1.14.3",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.1.8",
		"tailwindcss-bg-svg": "^2.0.0",
		"tsconfig": "latest",
		"tslib": "^2.3.1",
		"typescript": "~4.5.4",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.8.3",
		"@supabase/supabase-js": "^2.0.4",
		"@zerodevx/svelte-toast": "^1.0.0-rc.1",
		"clsx": "^1.2.1",
		"color2k": "^2.0.0",
		"crypto-js": "^4.1.1",
		"lodash": "^4.17.21",
		"plaid": "^12.1.0",
		"stripe": "^10.17.0",
		"svelte-typed-context": "^1.0.1"
	},
	"msw": {
		"workerDirectory": "static"
	}
}
