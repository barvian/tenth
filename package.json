{
	"name": "tenth",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "source .env && vite dev --port $DEV_SERVER_PORT",
		"dev:inngest": "source .env && npx inngest-cli dev -u http://localhost:${DEV_SERVER_PORT}/api/inngest",
		"dev:emails": "npx sveltemail preview",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:types": "npx inngest-cli types ts --output lib/inngest.d.ts && supabase gen types typescript --local > lib/supabase.d.ts",
		"reset:db": "supabase db reset",
		"test:db": "supabase test db",
		"postinstall": "patch-package",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@rollup/plugin-yaml": "^3.1.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.4",
		"@types/crypto-js": "^4.1.1",
		"@types/lodash": "^4.14.186",
		"@types/marked": "^4.0.8",
		"@types/nodemailer": "^6.4.7",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.8",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"husky": "^8.0.3",
		"inngest-cli": "^0.8.5",
		"lint-staged": "^13.1.0",
		"msw": "^1.0.0",
		"patch-package": "^6.4.7",
		"postcss": "^8.4.16",
		"prettier": "^2.5.1",
		"prettier-plugin-organize-imports": "^3.2.1",
		"prettier-plugin-svelte": "^2.5.0",
		"rimraf": "^3.0.2",
		"supabase": "^1.30.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.2.4",
		"tailwindcss-bg-svg": "^2.0.0",
		"tsconfig": "latest",
		"tslib": "^2.3.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@sentry/browser": "^7.28.1",
		"@sentry/node": "^7.28.1",
		"@stripe/stripe-js": "^1.46.0",
		"@supabase/auth-helpers-sveltekit": "^0.8.3",
		"@supabase/supabase-js": "^2.0.4",
		"@zerodevx/svelte-toast": "^1.0.0-rc.1",
		"clsx": "^1.2.1",
		"crypto-js": "^4.1.1",
		"inngest": "^0.9.0",
		"lodash": "^4.17.21",
		"marked": "^4.2.12",
		"mdsvex": "^0.10.6",
		"nodemailer": "^6.9.0",
		"ska": "file:../ska/package",
		"stripe": "^10.17.0",
		"svelte-typed-context": "^1.0.1",
		"sveltemail": "file:../sveltemail"
	},
	"msw": {
		"workerDirectory": "static"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": "eslint --cache --fix",
		"*.{js,ts,svelte,css,md}": "prettier --write"
	}
}
